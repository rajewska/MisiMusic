<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rajewska.misimusicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/misi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/misi"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SongsActivity"
            android:label="Songs"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ArtistsActivity" android:label="Artists"/>
        <activity android:name=".AlbumsActivity" android:label="Albums"/>
        <activity android:name=".PlaylistsActivity" android:label="Playlists"/>
        <activity android:name=".TagsActivity" android:label="Tags" />
        <activity android:name=".FavoritesActivity" android:label="Favorites"> </activity>
    </application>

</manifest>